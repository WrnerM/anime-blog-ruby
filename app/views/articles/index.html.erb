<%= link_to 'New', new_article_path %>

<table>
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Body</th>
        </tr>
    </thead>
    <tbody>
        <% @articles.each do |article| %>
            <tr>
                <td><%= article.id %></td>
                <td><%= article.name %></td>
                <td><%= article.body %></td>
                <td><%= button_to 'Delete', article_path(article.id), method: :delete %> </td>
                <td><%= link_to 'Update', article_path(article) %> </td>
                <td><%= link_to 'Show', article_path(article) %> </td>




            </tr>
        <% end %>
    </tbody>
</table>